{
	// Place your snippets for jade here. Each snippet is defined under a snippet name and has a prefix, body and 
	// description. The prefix is what is used to trigger the snippet and the body will be expanded and inserted. Possible variables are:
	// $1, $2 for tab stops, $0 for the final cursor position, and ${1:label}, ${2:another} for placeholders. Placeholders with the 
	// same ids are connected.
	// Example:
	// "Print to console": {
	// 	"prefix": "log",
	// 	"body": [
	// 		"console.log('$1');",
	// 		"$2"
	// 	],
	// 	"description": "Log output to console"
	// }

		"v-for": {
			"prefix": "_for",
			"body": [
				"v-for='(alumno, i) in alumnosEnEnfermeria', :key='i'$1"
			],
			"description": "Bucle for vue"
		},
		"v-if": {
			"prefix": "_if",
			"body": [
				"v-if='!datosAlumno.foto'$1"
			],
			"description": "Condicional if vue"
		},
		"@click": {
			"prefix": "_click",
			"body": [
				"@click='nameFunction()'$1"
			],
			"description": "Evento Click"
		},
		"@change": {
			"prefix": "_change",
			"body": [
				"@change='nameFunction()'$1"
			],
			"description": "Evento Change"
		},
		"interpolacion de variables": {
			"prefix": "_inter",
			"body": [
				"{{ dato$1 }}"
			],
			"description": "interpolacion de variables"
		}

}


